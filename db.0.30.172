;
; BIND data file for local loopback interface
;
$TTL	604800
@	IN	SOA	ns1.practicadaw.net. root.localhost. (
			      2		; Serial
			 604800		; Refresh
			  86400		; Retry
			2419200		; Expire
			 604800 )	; Negative Cache TTL
;


@	IN	NS	pc1.practicadaw.net.
@	IN	MX	1	correos.practicadaw.net.
133	IN	PTR	pc1.practicadaw.net.
136	IN	PTR	correos.practicadaw.net
134	IN	PTR	pc2.practicadaw.net.
135	IN	PTR	pc3.practicadaw.net.
